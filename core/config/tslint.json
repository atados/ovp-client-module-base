{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-react-hooks"
  ],
  "rules": {
    "interface-name": [true, "never-prefix"],
    "object-literal-sort-keys": false,
    "jsx-no-lambda": [false],
    "no-console": [true, "log"],
    "no-relative-imports": ["never"],
    "ordered-imports": false,
    "member-access": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["node_modules/**/*", "../node_modules/**/*"]
  }
}
