{
  "version": "0.0.8",
  "private": true,
  "scripts": {
    "dev": "sh base/core/bin/run-dev.sh",
    "debug": "NODE_OPTIONS=\"--inspect\" next dev",
    "build": "NODE_ENV=production yarn css && yarn generate:lang && next build",
    "start": "NODE_ENV=production next start",
    "lint": "tslint \"./**/*.{ts,tsx}\" -c tslint.json",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "css": "sh ./base/core/bin/build-css.sh",
    "deploy": "yarn type-check && yarn deploy:now",
    "deploy:now": "now -e NOW_GITHUB_COMMIT_SHA=$(git rev-parse HEAD) -e NOW_GITHUB_COMMIT_DIRTY=$(if [[ $(git diff --stat) != '' ]]; then echo 'true'; else echo 'false' ; fi)",
    "prettier": "prettier --write \"{next.config.js,gulpfile.js,{pages,base,channel,__{tests,mocks}__}/**/*.{js,ts,tsx}}\"",
    "export:config": "tncc base/core/bin/export-configuration-as-json.ts -s --run-args \"--run\"",
    "check:lang": "ts-node -P tsconfig.server.json -e \"require('./base/core/show-lang-status').default()\"",
    "generate:lang": "ts-node -P tsconfig.server.json -e \"require('./base/core/generate-intl-messages-files').default()\"",
    "sync": "yarn sync:config && yarn sync:pages && yarn sync:static",
    "sync:pages": "yarn export:config && node base/core/bin/sync-pages-map.js",
    "sync:config": "node base/core/bin/sync-configuration.js",
    "sync:static": "node base/core/bin/sync-static-files.js",
    "build:lang": "next build",
    "shade-color": "ts-node -P tsconfig.server.json ./base/core/bin/shade-color -q",
    "generate:channel.css": "ts-node -P tsconfig.server.json -e \"require('./base/core/generate-css').default()\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && if [ -d \"base/core\" ]; then node base/core/bin/check-base-submodule-has-changes.js && yarn type-check && lint-staged; else yarn type-check && yarn lint; fi"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "git add"
    ],
    "**/*.{ts,tsx}": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ]
  },
